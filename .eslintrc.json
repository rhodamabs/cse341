{
    "env": {
        "node": true,
        "es2021": true,
        "browser":true,
        "commonjs":true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb", "prettier" ],

    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars":"warn",
        "no-console":"off",
        "func-names":"off",
        "no-process-exit":"off",
        "object-shorthand":"off",
        "class-methods-use-this":"off",
        "linebreak-style":["error",  "windows"]



    }
}
